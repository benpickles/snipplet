<% content_for :title do %>Login<% end %>

<% form_for @user_session do |f| %>
  <div>
    <%= f.label :username %>
    <%= f.text_field :username %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= submit_tag 'Login' %>
  </div>
<% end %>
